{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "react": [
        "./node_modules/@raycast/api/node_modules/@types/react/index.d.ts"
      ],
      "react/jsx-runtime": [
        "./node_modules/@raycast/api/node_modules/@types/react/jsx-runtime.d.ts"
      ],
      "react/jsx-dev-runtime": [
        "./node_modules/@raycast/api/node_modules/@types/react/jsx-dev-runtime.d.ts"
      ]
    },
    "typeRoots": [
      "./node_modules/@raycast/api/node_modules/@types"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src",
    "raycast-env.d.ts"
  ],
  "exclude": [
    "src/**/*.test.ts"
  ]
}
